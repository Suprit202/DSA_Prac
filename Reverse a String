// User function Template for C
char* reverseString(char* s) {
    // Code Here
    int n = strlen(s);
    
    if(n <= 1){
        return s;
    }
    
    for(int i = 0; i<n; i++){
        if(i < n-i-1){
            int temp = s[i];
            s[i] = s[n-1-i];
            s[n-1-i] = temp;
        }
        else{
            break;
        }
    }
    
    return s;
}
